<h1>Listing salary_items</h1>

<table>
  <tr>
    <th>Code</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @salary_items.each do |salary_item| %>
  <tr>
    <td><%= salary_item.code %></td>
    <td><%= salary_item.name %></td>
    <td><%= link_to 'Show', salary_item %></td>
    <td><%= link_to 'Edit', edit_salary_item_path(salary_item) %></td>
    <td><%= link_to 'Destroy', salary_item, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

